generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model firehose {
  id          Int      @id @default(autoincrement())
  title       String
  url         String   @unique
  description String?
  postdate    DateTime
  source      String?
  slug        String?  @unique @default("")
}
